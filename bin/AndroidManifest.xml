<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raipeng.appAB"
    android:versionCode="1"
    android:versionName="1.0" >
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:name="com.raipeng.appAB.control.BaseApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.raipeng.appAB.control.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"  >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
             android:name="com.raipeng.appAB.control.custommanage.ContactsBookActivity"  
            android:screenOrientation="portrait" />
         <activity 
             android:name="com.raipeng.appAB.control.SettingActivity"  
            android:screenOrientation="portrait" />
          <activity 
             android:name="com.raipeng.appAB.control.myshop.AddNewProduct"  
            android:screenOrientation="portrait" />
          <activity 
             android:name="com.raipeng.appAB.control.myshop.AddProductColor"  
            android:screenOrientation="portrait" />
    </application>

</manifest>
